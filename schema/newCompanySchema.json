{
    "$id": "http://example.com/example.json",
    "type": "object",
    "additionalProperties": false,
    "properties": {
      "name": {
        "$id": "/properties/name",
        "type": "string",
        "title": "The Name Schema ",
        "default": "",
        "minLength": 1,
        "maxLength": 55,
        "examples": ["Terry"]
      },
      "handle": {
        "$id": "/properties/handle",
        "type": "string",
        "title": "The Handle Schema ",
        "default": "",
        "minLength": 1,
        "maxLength": 55,
        "examples": ["Terry Inc"]
      },
      "logo": {
        "$id": "/properties/logo",
        "type": "string",
        "title": "The Logo Schema ",
        "default": "",
        "format": "uri",
        "examples": [
          "https://terry.jpg"
        ]
      },
      "_token": {
        "$id": "/properties/token",
        "type": "string",
        "title": "The Token Schema ",
        "default": "",
        "examples": [
          "foobar123"
        ]
      }
    },
    "required": ["name", "handle"]
  }
  